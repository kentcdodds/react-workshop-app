{
  "name": "@kentcdodds/react-workshop-app",
  "version": "0.0.0-semantically-released",
  "publishConfig": {
    "access": "public"
  },
  "description": "An abstraction for all my React workshops",
  "main": "dist/@kentcdodds/react-workshop-app.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": "Kent C. Dodds <me@kentcdodds.com> (https://kentcdodds.com)",
  "license": "MIT",
  "engines": {
    "node": ">=10.13",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/react-workshop-app"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/react-workshop-app/issues"
  },
  "homepage": "https://github.com/kentcdodds/react-workshop-app#readme",
  "files": [
    "dist",
    "codegen.js",
    "setup-tests.js",
    "dev-tools.js",
    "dev-tools-build",
    "server.js",
    "test-utils.js"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:build": "npm run build -- --watch",
    "dev:examples": "cd ./example/react-fundamentals && npm start",
    "setup": "concurrently \"npm:setup:*\"",
    "setup:main": "npm install && npm run validate -s",
    "setup:examples": "cd ./example/react-fundamentals && npm run setup",
    "build": "npm run build:main && concurrently \"npm:build:bundle:*\"",
    "build:main": "kcd-scripts build",
    "build:bundle:main": "kcd-scripts build --bundle esm --no-clean",
    "build:bundle:dev-tools": "cross-env BUILD_INPUT=\"src/dev-tools/*.{ts,tsx}\" BUILD_FILENAME_PREFIX=\"dev-tools-build\" kcd-scripts build --bundle esm --no-clean",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "typecheck": "kcd-scripts typecheck",
    "test:debug": "node --inspect-brk node_modules/kcd-scripts/dist/scripts/test.js --runInBand --no-cache",
    "validate": "kcd-scripts validate"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@emotion/core": "^10.0.35",
    "@reach/tabs": "^0.12.1",
    "@reach/tooltip": "^0.12.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "chalk": "^4.1.0",
    "codegen.macro": "^4.1.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "emotion-theming": "^10.0.27",
    "facepaint": "^1.2.1",
    "focus-visible": "^5.2.0",
    "glob": "^7.1.6",
    "history": "^4.10.1",
    "inquirer": "^7.3.3",
    "is-ci": "^2.0.0",
    "mdx-loader": "^3.0.2",
    "mq-polyfill": "^1.1.8",
    "msw": "^0.25.0",
    "node-match-path": "^0.6.0",
    "normalize.css": "^8.0.1",
    "raw-loader": "^4.0.2",
    "react-error-boundary": "^3.1.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "replace-in-file": "^6.1.0"
  },
  "devDependencies": {
    "@types/facepaint": "^1.2.1",
    "@types/history": "^4.7.8",
    "@types/preval.macro": "^3.0.0",
    "@types/react-router-dom": "^5.1.7",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "identity-obj-proxy": "^3.0.0",
    "kcd-scripts": "^7.5.5",
    "postcss-cli": "^7.1.2",
    "preval.macro": "^5.0.0",
    "react": "0.0.0-experimental-e5d06e34b",
    "react-dom": "0.0.0-experimental-e5d06e34b",
    "resolve": "^1.19.0",
    "typescript": "^4.1.3",
    "whatwg-fetch": "^3.5.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "no-console": "off",
      "max-lines-per-function": "off",
      "guard-for-in": "off",
      "react/display-name": "off",
      "react/prop-types": "off",
      "import/no-unresolved": "off"
    }
  }
}
